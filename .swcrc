{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "baseUrl": "./src",
    "paths": {
      "@application/*": [
        "application/*"
      ],
      "@domain/*": [
        "domain/*"
      ],
      "@infra/*": [
        "infra/*"
      ]
    }
  },
  "module": {
    "type": "commonjs"
  }
}