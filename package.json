{
  "name": "ikatoo-backend",
  "author": {
    "name": "Milton Carlos Katoo",
    "email": "mckatoo@gmail.com",
    "url": "https://ikatoo.com.br"
  },
  "version": "1.0.0",
  "main": "index.js",
  "license": "CC0-1.0",
  "scripts": {
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint --fix src",
    "test": "rimraf sqlite.db && NODE_ENV=test jest",
    "test:coverage": "rimraf sqlite.db,coverage && NODE_ENV=test jest ./src --coverage",
    "test:express": "rimraf sqlite.db && NODE_ENV=test jest src/infra/http/express",
    "test:domain": "rimraf sqlite.db && NODE_ENV=test jest src/domain",
    "test:application": "rimraf sqlite.db && NODE_ENV=test jest src/application",
    "test:infra": "rimraf sqlite.db && NODE_ENV=test jest src/infra",
    "express": "ts-node -r tsconfig-paths/register src/infra/http/express/index.ts",
    "dev": "NODE_ENV=test tsnd -r tsconfig-paths/register --watch src/ --respawn --transpile-only src/infra/http/express/index.ts",
    "dev:express": "NODE_ENV=test tsnd -r tsconfig-paths/register src/infra/http/express/index.ts",
    "start": "yarn express"
  },
  "devDependencies": {
    "@swc/core": "1.2.203",
    "@swc/jest": "0.2.21",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.2",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "18.7.11",
    "@types/sqlite3": "3.1.8",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "cross-env": "7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "22.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "import-sort-cli": "6.0.0",
    "import-sort-parser-babylon": "6.0.0",
    "import-sort-parser-typescript": "6.0.0",
    "import-sort-style-renke": "6.0.0",
    "jest": "29.0.2",
    "rimraf": "3.0.2",
    "sqlite": "4.1.2",
    "sqlite3": "5.0.11",
    "supertest": "6.2.3",
    "ts-node": "10.8.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.7.3"
  },
  "dependencies": {
    "bcrypt": "5.0.1",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "express-async-errors": "3.1.1",
    "jsonwebtoken": "8.5.1"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint"
    }
  }
}
